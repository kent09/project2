{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "PsychicCat/monero-php": "*",
        "bacon/bacon-qr-code": "1.0.3",
        "barryvdh/laravel-cors": "^0.11.2",
        "block_io-php/block_io-php": "^1.2",
        "darkaonline/swagger-lume": "5.6.*",
        "doctrine/dbal": "^2.8",
        "firebase/php-jwt": "^5.0",
        "flipbox/lumen-generator": "^5.6",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/mail": "^5.6",
        "illuminate/redis": "^5.6",
        "illuminate/routing": "5.6",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "jenssegers/agent": "^2.6",
        "laravel/lumen-framework": "5.6.*",
        "laravel/socialite": "^3.0",
        "laravelista/lumen-vendor-publish": "^2.1",
        "nblackburn/laravel-hashids": "^1.2",
        "paypal/rest-api-sdk-php": "^1.13",
        "pragmarx/google2fa": "^3.0",
        "predis/predis": "^1.1",
        "smskin/lumen-make": "^1.0",
        "spatie/emoji": "^1.0",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/UtilHelper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
